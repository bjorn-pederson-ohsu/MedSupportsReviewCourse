package module{	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.events.MouseEvent;	import com.reflektii.db.dbxml;	public class moduleController2 extends EventDispatcher {		private var _model:Object;		private var _xmlFetch:dbxml;		public function moduleController2(m:Object) {			// constructor code			_model = m;		}		public function loadXml():void {			var url:String = "overview.xml";			_xmlFetch = new dbxml(url);			_xmlFetch.addEventListener(dbxml.UPDATE,update);		}		public function update(event:Event):void {			_model.xml = _xmlFetch.xmlreturn;			_xmlFetch.removeEventListener(dbxml.UPDATE,update);		}		public function buildMenu(i:int,v:int,j:int):void {			_model.menuCalc(i,v,j);		}		public function buildSubMenu(i:int,v:int,j:int):void {			_model.subMenuCalc(i,v,j);		}		public function menuEvent(event:Event):void {			//trace("From controller: "+event.target.name, event.target.buttonNum);			if (event.target.name == "menu")			{				if (event.target.buttonNum == 0)				{					_model.returnToMain(event.target.buttonNum);				}				if (event.target.buttonNum>0){					_model.changeMenu(event.target.buttonNum)				}			}			if (event.target.name == "submenu")			{				trace("From controller: "+event.target.name, event.target.buttonNum);				_model.subMenuUpdate(event.target.buttonNum)			}		}		public function removalComplete():void {			_model.returnLanding();		}	}}