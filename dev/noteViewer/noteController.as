package noteViewer{	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.events.MouseEvent;	import com.reflektii.db.dbxml;		public class noteController extends EventDispatcher {		private var _model:Object;		private var _xmlFetch:dbxml;		public function noteController(m:Object) {			// constructor code			_model = m;		}		public function loadXml():void {			var url:String = "notes.xml";			_xmlFetch = new dbxml(url);			_xmlFetch.addEventListener(dbxml.UPDATE,update);		}		public function update(event:Event):void {						_model.setXml(_xmlFetch.xmlreturn);			_xmlFetch.removeEventListener(dbxml.UPDATE,update);		}				public function downGallery(i:int):void {			_model.moveGallery("down",i)		}		public function upGallery(i:int):void {			_model.moveGallery("up",i)		}				public function noteSection(i:int): void{			trace ("From Controller: "+i)			_model.sectionChange(i)		}			}}