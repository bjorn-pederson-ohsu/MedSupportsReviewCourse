package infoViewer{	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.events.MouseEvent;	public class infoController extends EventDispatcher {		private var _model:Object;		public function infoController(m:Object) {			// constructor code			_model = m;		}		public function setXml(xml:XML):void {			_model.setXml(xml);		}				public function downGallery(i:int):void {			_model.moveGallery("down",i)		}		public function upGallery(i:int):void {			_model.moveGallery("up",i)		}			}}