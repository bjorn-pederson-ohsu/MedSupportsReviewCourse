package buttonBuild{	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.display.MovieClip;	import caurina.transitions.Tweener;	import caurina.transitions.properties.FilterShortcuts;	import flash.events.MouseEvent;	public class goButton extends MovieClip {		//private var tween:Tweener		//private var filter:FilterShortcuts		public static var UPDATE:String = 'ButtonClicked';		private var button:redArrow		public function goButton() {			// constructor code			FilterShortcuts.init();			goButtonBuild();		}		private function goButtonBuild():void {			button = new redArrow();			button.alpha=.5			addChild(button);			button.buttonMode=true			button.mouseChildren = false;			button.useHandCursor = true;			button.addEventListener(MouseEvent.MOUSE_OVER,buttonOver);			button.addEventListener(MouseEvent.MOUSE_OUT, buttonOut);			button.addEventListener(MouseEvent.MOUSE_DOWN, buttonDown);			button.addEventListener(Event.REMOVED_FROM_STAGE, removedFromStage);		}		private function buttonOver(event:MouseEvent):void {			Tweener.addTween(button,{alpha:1,time:.35,_Glow_alpha:.65,_Glow_blurX:15,_Glow_blurY:15,_Glow_color:0x00CC00,_Glow_inner:false});		}		private function buttonOut(event:MouseEvent):void {			Tweener.addTween(button,{alpha:.5,time:.35,_Glow_alpha:0,_Glow_blurX:0,_Glow_blurY:0,_Glow_color:0x00CC00,_Glow_inner:false});		}		private function buttonDown(event:MouseEvent):void {			dispatchEvent(new Event(goButton.UPDATE));		}		private function removedFromStage(event:Event){			button.removeEventListener(MouseEvent.MOUSE_OVER,buttonOver);			button.removeEventListener(MouseEvent.MOUSE_OUT, buttonOut);			button.removeEventListener(MouseEvent.MOUSE_DOWN, buttonDown);			button.removeEventListener(Event.REMOVED_FROM_STAGE, removedFromStage);			button=null		}	}}