package buttonBuild{	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.display.MovieClip;	import caurina.transitions.Tweener;	import caurina.transitions.properties.FilterShortcuts;	import flash.events.MouseEvent;	public class supportButtonSet extends MovieClip {		//private var tween:Tweener		//private var filter:FilterShortcuts		public static var UPDATE:String = 'ButtonClicked';		private var button:supportButton;		private var buttonTitle:String = "";		public function supportButtonSet(s:String) {			// constructor code			buttonTitle = s;			FilterShortcuts.init();			supportButtonBuild();		}		private function supportButtonBuild():void {			button = new supportButton();			button.buttonLabel.text = buttonTitle;			Tweener.addTween(button.bg,{alpha:.75,_Glow_alpha:.75,_Glow_blurX:10,_Glow_blurY:10,_Glow_color:0x6c6c6a,_Glow_inner:false,transition:"easeOutQuad"});			addChild(button);			button.buttonMode=true			button.mouseChildren = false;			button.useHandCursor = true;			button.addEventListener(MouseEvent.MOUSE_OVER,buttonOver);			button.addEventListener(MouseEvent.MOUSE_OUT, buttonOut);			button.addEventListener(MouseEvent.MOUSE_DOWN, buttonDown);			button.addEventListener(Event.REMOVED_FROM_STAGE, removedFromStage);		}		private function buttonOver(event:MouseEvent):void {			Tweener.addTween(button.bg,{alpha:1,time:.35,_Glow_alpha:.75,_Glow_blurX:15,_Glow_blurY:15,_Glow_color:0xffffff,_Glow_inner:false,transition:"easeOutQuad"});		}		private function buttonOut(event:MouseEvent):void {			Tweener.addTween(button.bg,{alpha:.75,time:.35,_Glow_alpha:.75,_Glow_blurX:10,_Glow_blurY:10,_Glow_color:0x6c6c6a,_Glow_inner:false,transition:"easeOutQuad"});		}		private function buttonDown(event:MouseEvent):void {			dispatchEvent(new Event(supportButtonSet.UPDATE));		}		private function removedFromStage(event:Event){			button.removeEventListener(MouseEvent.MOUSE_OVER,buttonOver);			button.removeEventListener(MouseEvent.MOUSE_OUT, buttonOut);			button.removeEventListener(MouseEvent.MOUSE_DOWN, buttonDown);			button.removeEventListener(Event.REMOVED_FROM_STAGE, removedFromStage);			button=null		}	}}