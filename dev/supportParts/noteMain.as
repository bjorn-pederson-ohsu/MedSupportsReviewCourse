package supportParts{	import flash.display.MovieClip;	import flash.events.EventDispatcher;	import flash.events.Event;	import noteViewer.*;	import caurina.transitions.Tweener;	public class noteMain extends MovieClip {				private var model:noteModel		private var view:noteView		private var control:noteController				private var bgwidth:int;		private var bgheight:int;		public function noteMain(i:int,v:int) {			// constructor code			bgwidth = i;			bgheight = v;			addEventListener(Event.ADDED_TO_STAGE,addedtostage);		}		private function addedtostage(event:Event):void {			removeEventListener(Event.ADDED_TO_STAGE,addedtostage);			addEventListener(Event.REMOVED_FROM_STAGE,removedFromStage);			model= new  noteModel();			control = new noteController(model);			trace("from helpMain: "+bgwidth, bgheight)			view = new noteView(model,control,bgwidth, bgheight);			addChild(view);					}		private function removedFromStage(event:Event):void {			removeEventListener(Event.REMOVED_FROM_STAGE,removedFromStage);			removeChild(view)		}	}}