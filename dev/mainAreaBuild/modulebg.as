package mainAreaBuild{	import flash.events.Event;	import flash.display.Sprite;	import flash.display.Shape;	import flash.display.CapsStyle;    import flash.display.JointStyle;	public class modulebg extends Sprite {		//private var tween:Tweener		//private var filter:FilterShortcuts		//private var mainArea:subMenumainArea;		private var myFrame:Shape;		private var mainArea:Sprite;		private var bg:Sprite;		private var bgFrame:Sprite;		private var heightNum:int;		private var widthNum:int;		public function modulebg(i:int, v:int) {			// constructor code			heightNum = i;			widthNum = v;			addEventListener(Event.ADDED_TO_STAGE,modBgBuild);		}		private function modBgBuild(event:Event):void {			removeEventListener(Event.ADDED_TO_STAGE,modBgBuild);			mainArea = new Sprite() ;			addChild(mainArea);			bg=new Sprite ();			mainArea.addChild(bg);			myFrame = new Shape() ;			myFrame.graphics.moveTo((widthNum-10),0);			myFrame.graphics.lineStyle(1, 0x7d7d7d, 1, true,"normal", CapsStyle.ROUND, JointStyle.ROUND, 0);			myFrame.graphics.beginFill(0xf4f4d8,.4);			myFrame.graphics.lineTo(10, 0);			myFrame.graphics.curveTo(0,0,0,10);			myFrame.graphics.lineTo(0, (heightNum-10));			myFrame.graphics.curveTo(0,heightNum,10,heightNum);			myFrame.graphics.lineTo((widthNum-10), heightNum);			myFrame.graphics.curveTo(widthNum,heightNum,widthNum,(heightNum-10));			myFrame.graphics.lineTo(widthNum, 10);			myFrame.graphics.curveTo(widthNum,0,(widthNum-10),0);			myFrame.graphics.endFill();			bg.addChild(myFrame);			bg.cacheAsBitmap=true		}	}}