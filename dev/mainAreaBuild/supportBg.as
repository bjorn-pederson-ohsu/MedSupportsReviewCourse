package mainAreaBuild{	import flash.events.Event;	import flash.display.Sprite;	import flash.display.Shape;	import flash.display.CapsStyle;    import flash.display.JointStyle;	import flash.geom.Rectangle	public class supportBg extends Sprite {		//private var tween:Tweener		//private var filter:FilterShortcuts		//private var mainArea:subMenumainArea;		private var myFrame:Shape;		private var mainArea:Sprite;		private var bg:Sprite;		private var bgFrame:Sprite;		private var heightNum:int;		private var widthNum:int;		public function supportBg(i:int, v:int) {			// constructor code			heightNum = i;			widthNum = v;			addEventListener(Event.ADDED_TO_STAGE,supBgBuild);		}		private function supBgBuild(event:Event):void {			removeEventListener(Event.ADDED_TO_STAGE,supBgBuild);			//mainArea = new Sprite() ;			//addChild(mainArea);			bg=new Sprite ();			//mainArea.addChild(bg);			addChild(bg);			myFrame = new Shape() ;			myFrame.graphics.moveTo((widthNum-10),0);			myFrame.graphics.lineStyle(1, 0xffffff, .5, true,"normal", CapsStyle.ROUND, JointStyle.ROUND, 0);			myFrame.graphics.beginFill(0xB9D8DD,.6);			myFrame.graphics.lineTo(5, 0);			myFrame.graphics.curveTo(0,0,0,5);			myFrame.graphics.lineTo(0, (heightNum-5));			myFrame.graphics.curveTo(0,heightNum,10,heightNum);			myFrame.graphics.lineTo((widthNum-5), heightNum);			myFrame.graphics.curveTo(widthNum,heightNum,widthNum,(heightNum-5));			myFrame.graphics.lineTo(widthNum, 5);			myFrame.graphics.curveTo(widthNum,0,(widthNum-5),0);			myFrame.graphics.endFill();			//myFrame.scale9Grid = new Rectangle(5, 5, widthNum-10, heightNum-10);			bg.addChild(myFrame);			bg.cacheAsBitmap=true		}	}}