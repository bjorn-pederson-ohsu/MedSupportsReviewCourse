package testViewer{	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.display.MovieClip;	import caurina.transitions.Tweener;	import flash.events.MouseEvent;	public class progressArea extends MovieClip {		private var prog:progressBar;		private var _model:Object;		public function progressArea(m:Object) {			// constructor code			_model = m			progBuild();					}		private function progBuild():void {			prog = new progressBar();			prog.alpha = .35			prog.square.width=0			addChild(prog);			_model.addEventListener('progressbarupdate', moveBar) 			prog.addEventListener(Event.REMOVED_FROM_STAGE, removedFromStage);		}		private function moveBar(event:Event):void {			var percentage:int =Math.ceil(400*(_model.traceout()));			Tweener.addTween(prog.square, {width:percentage, time:.75, delay:0, transition:"easeInBounce"});		}		private function removedFromStage(event:Event) {			_model.removeEventListener('progressbarupdate', moveBar) 			prog.removeEventListener(Event.REMOVED_FROM_STAGE, removedFromStage);			prog = null;		}	}}