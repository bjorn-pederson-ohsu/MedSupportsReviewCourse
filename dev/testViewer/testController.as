package testViewer{	import flash.events.Event;	import flash.events.EventDispatcher;	import flash.events.MouseEvent;		public class testController extends EventDispatcher {		private var _model:Object;		public function testController(m:Object) {			// constructor code			_model = m;		}				public function setXml(xml:XML):void{			_model.setXml(xml)		}				public function startTest():void{			_model.questionBuild()		}				public function answerCheck(i:int):void {			_model.answerCheck(i)		}						public function testSection(i:int): void{			trace ("From Controller: "+i)			_model.sectionChange(i)		}			}}